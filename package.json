{
    "name": "satford",
    "version": "0.0.1",
    "description": "Simple API testing service for devs",
    "main": "index.js",
    "keywords": [
        "api",
        "testing"
    ],
    "license": "MIT",
    "author": "Marco Busemann",
    "engines": {
        "node": ">=6.9.5"
    },
    "scripts": {
        "clean": "rimraf dist",
        "lint": "tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx'",
        "build-server": "tsc -p ./src/server",
        "build-manualtests": "tsc -p ./src/tests",
        "build": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER\" \"npm run build-server\"",
        "test-server": "jest --config=./jest-config-server.json",
        "test": "npm run test-server",
        "dev": "concurrently --prefix \"[{name}]\" --names \"DOCKER\" \"docker-compose -f docker/debug/docker-compose.yml up\""
    },
    "devDependencies": {
        "typescript": "^2.6.2",
        "concurrently": "^3.4.0",
        "jest": "^21.2.1",
        "onchange": "^3.3.0",
        "rimraf": "^2.6.1",
        "style-loader": "^0.16.1",
        "ts-jest": "^21.1.3",
        "tslint": "^5.1.0",
        "cross-env": "^4.0.0"
    },
    "dependencies": {
        "@types/agenda": "^1.0.0",
        "@types/express": "^4.0.39",
        "@types/lodash": "^4.14.88",
        "@types/mongodb": "^2.2.17",
        "@types/pubsub-js": "^1.5.18",
        "@types/react": "^16.0.30",
        "@types/react-dom": "^16.0.3",
        "@types/request": "^2.0.8",
        "@types/socket.io": "^1.4.31",
        "agenda": "^1.0.3",
        "agendash": "^0.4.0",
        "express": "^4.16.2",
        "lodash": "^4.17.4",
        "moment": "^2.19.4",
        "mongodb": "^3.0.0-rc0",
        "node-mattermost": "0.0.1",
        "ping-wrapper2": "0.0.6",
        "pubsub-js": "^1.5.7",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "request": "^2.83.0",
        "socket.io": "^2.0.4"
    }
}
